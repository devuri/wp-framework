{% extends "base.html.twig" %}

{% block content %}
<h1>Welcome to {{ site.name }}</h1>
<p>{{ site.description }}</p>
<ul>
    {% for post in archive.posts %}
        <li>
            <a href="{{ post.url }}">{{ post.title }}</a>
            <p>{{ post.excerpt }}</p>
        </li>
    {% endfor %}
</ul>
{% endblock %}